{
    "name": "thezombieguy/laravel-internal-request",
    "description": "A Laravel package to handle internal route requests",
    "keywords": ["laravel", "internal", "route", "request", "internal route"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/http": "^9.0|^10.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.0",
        "phpstan/phpstan": "^1.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.4",
        "spatie/phpunit-watcher": "^1.0",
        "nunomaduro/collision": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "thezombieguy\\InternalRequest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "thezombieguy\\InternalRequest\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "thezombieguy\\InternalRequest\\InternalRequestServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}